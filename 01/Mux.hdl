/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
   Not (a = sel, b = sel, out = notsel);
   Nand (a = notsel, b = a, out = anothera);
   Nand (a = sel, b = b, out = anotherb);
   Nand (a = anothera, b = anotherb, out = out);
}